---
export async function getStaticPaths() {
	const posts = await Astro.glob('../pages/*.md');
	return posts.map((post) => {
		return {
			params: {slug: post.frontmatter.slug},
			props: {...post}
		}
	})
}


const {Content, frontmatter} = Astro.props;

---

<article>
    <h1>{frontmatter.title}</h1>
    <h2>hej</h2>
    <Content />
</article>